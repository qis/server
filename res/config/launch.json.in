{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "@PROJECT_NAME@",
      "type": "cppvsdbg",
      "request": "launch",
      "preLaunchTask": "build",
      "internalConsoleOptions": "openOnSessionStart",
      "program": "@CMAKE_CURRENT_BINARY_DIR@/@PROJECT_NAME@@CMAKE_EXECUTABLE_SUFFIX@",
      "args": [ "@PROJECT_NAME@.ini" "." ],
      "cwd": "${workspaceRoot}",
      "externalConsole": false,
      "stopAtEntry": false,
      "environment": []
    }
  ]
}
